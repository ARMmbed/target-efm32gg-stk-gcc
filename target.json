{
  "name": "stk3700-efm32gg-gcc",
  "version": "0.0.5",
  "inherits": {
    "mbed-gcc": "*"
  },
  "description": "GCC target description for the STK3700 dev board.",
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "private": true,
  "keywords": [
    "mbed-target:efm32gg",
    "gcc"
  ],
  "similarTo": [
    "stk3700",
    "efm32gg990f",
    "efm32gg",
    "efm32",
    "silabs",
    "cortex-m3",
    "armv7-m",
    "arm",
    "gcc"
  ],
  "toolchain": "CMake/toolchain.cmake",
  "debug-server": [
    "JLinkGDBServer",
    "-if",
    "SWD",
    "-speed",
    "4000",
    "-device",
    "EFM32GG990F1024",
    "-log",
    "/tmp/jlink-efm.log"
  ],
  "debug": [
    "arm-none-eabi-gdb",
    "$program",
    "--eval",
    "target remote localhost:2331",
    "-ex",
    "mon device EFM32GG990F1024",
    "-ex",
    "mon flash download 1",
    "-ex",
    "mon SWO Start 0 875000Hz"
  ]
}