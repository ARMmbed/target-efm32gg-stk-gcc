{
  "name": "efm32gg-stk-gcc",
  "version": "0.3.3",
  "description": "Official mbed build target for the mbed EFM32GG STK3700 development board, using the GCC toolchain.",
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "inherits": {
    "mbed-gcc": "~0.1.0"
  },
  "keywords": [
    "mbed-target:efm32gg-stk",
    "mbed-official",
    "efm32gg",
    "efm32gg-stk",
    "gcc"
  ],
  "similarTo": [
    "efm32gg-stk",
    "efm32gg",
    "efm32",
    "silabs",
    "cortex-m3",
    "armv7-m",
    "arm",
    "gcc"
  ],
  "config": {
    "cmsis": {
      "nvic": {
        "ram_vector_address": "(&__start_vector_table__)",
        "flash_vector_address": "0x0",
        "user_irq_offset": 16,
        "user_irq_number": 39
      }
    },
    "uvisor": {
      "present": false
    },
    "hardware": {
      "clock": {
        "core": {
          "source": "HFXO",
          "frequency": 48000000
        },
        "low-energy": {
          "source": "LFXO",
          "frequency": 32768
        }
      },
      "pins": {
        "LED0": "PE2",
        "LED1": "PE3",
        "LED2": "LED0",
        "LED3": "LED0",
        "LED4": "LED1",
        "SW0": "PB9",
        "SW1": "PB10",
        "BTN0": "SW0",
        "BTN1": "SW1",
        "UART_TX": "PD0",
        "UART_RX": "PD1",
        "SERIAL_TX": "PD0",
        "SERIAL_RX": "PD1",
        "STDIO_UART_TX": "PE0",
        "STDIO_UART_RX": "PE1",
        "USBTX": "PE0",
        "USBRX": "PE1",
        "EFM_BC_EN": "PF7",
        "SLIDER_PAD_0": "PC8",
        "SLIDER_PAD_1": "PC9",
        "SLIDER_PAD_2": "PC10",
        "SLIDER_PAD_3": "PC11"
      },
      "test-pins": {
        "spi": {
          "MOSI": "PD0",
          "MISO": "PD1",
          "SCLK": "PD2",
          "SSEL": "PD3"
        },
        "i2c": {
          "SDA": "PC4",
          "SCL": "PC5"
        }
      },
      "lesense": {
        "port": "gpioPortC",
        "total-channels": 16
      }
    },
    "modules": {
      "analogin": {
        "size": 1
      },
      "analogout": {
        "size": 1
      },
      "gpio": {
        "size": 1
      },
      "i2c": {
        "size": 2
      },
      "pwmout": {
        "size": 1
      },
      "serial": {
        "size": 7,
        "stdio-uart": "UART0"
      },
      "spi": {
        "size": 3,
        "transaction-queue-size": 4
      }
    }
  },
  "toolchain": "CMake/toolchain.cmake",
  "debug-server": [
    "JLinkGDBServer",
    "-if",
    "SWD",
    "-speed",
    "4000",
    "-device",
    "EFM32GG990F1024",
    "-log",
    "/tmp/jlink-efm.log"
  ],
  "scripts": {
    "debug": [
      "valinor",
      "-t",
      "uvision",
      "--target",
      "efm32gg-stk",
      "$program"
    ],
    "test": [
      "mbed_test_wrapper",
      "--target",
      "EFM32GG_STK3700",
      "$program"
    ]
  }
}
